<view class="input-div">
	<input class="input-block" value="" placeholder="请输入" placeholder-style="color:#ccc" />
	<icon class="icon-search" type="search" />
</view>
<view class="container">
	<view class="scroll-content">
		<view wx:for="{{articleList}}" wx:key="index" id="{{item.id}}" class="ask" bindtap="openDetail">
			<view class="ask-title">{{item.title}}</view>
			<view class="ask-content">{{item.content}}</view>
		</view>
	</view>
</view>